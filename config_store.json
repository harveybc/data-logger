                "tables": [
                    {
                        "name": "normalizer",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "unbiaser",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "trimmer",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "feature_selector",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "cleaner",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    }

                ]
            },
            {
                "name": "feature_extractor",
                "tables": [
                    {
                        "name": "fe_ann",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "fe_cnn",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "fe_lstm",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "fe_transformer",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "fe_bi-lstm",
                        "columns": [
                            {
                                "name": "json_config",
                                "col_type": "String"
                            },
                            {
                                "name": "json_result",
                                "col_type": "String"
                            }
                        ]
                    },
                    {
                        "name": "fe_ann_training_error",
                        "columns": [
                            { "name" : "mse" },
                            { "name" : "mae" },
                            { "name" : "r2" },
                            { "name" : "interface_size", "col_type" : "Integer"},
                            {
                                "name" : "fe_ann_id",
                                "foreign_key" : "fe_ann.id"
                            }
                        ]
                    }
                ]
            },

            {
                "name": "evaluator",
                "tables": [
                    {
                        "name": "evaluations",
                        "columns": [
                            {
                                "name": "client_id",
                                "col_type": "integer"
                            },
                            {
                                "name": "data",
                                "col_type": "Text"
                            },
                            {
                                "name": "window_size",
                                "col_type": "integer"
                            },
                            {
                                "name": "feature_extractor_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "champion_genome_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "neat_config_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "evaluation_status",
                                "col_type": "string",
                                "length": 20,
                                "default": "pending"
                            },
                            {
                                "name": "json_result",
                                "col_type": "text"
                            },
                            {
                                "name": "data_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "feature_extracted_data_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "result_hash",
                                "col_type": "string",
                                "length": 64
                            }
                        ]
                    }
                ]
            },
            {
                "name": "optimizer",
                "tables": [
                    {
                        "name": "optimizations",
                        "columns": [
                            {
                                "name": "optimizer_name",
                                "col_type": "string",
                                "length": 100
                            },
                            {
                                "name": "optimization_algorithm",
                                "col_type": "string",
                                "length": 100
                            },
                            {
                                "name": "optimization_parameters",
                                "col_type": "text"
                            },
                            {
                                "name": "last_optimum_hash",
                                "col_type": "string",
                                "length": 64
                            },
                            {
                                "name": "status",
                                "col_type": "string",
                                "length": 20,
                                "default": "active"
                            }
                        ]
                    },
                    {
                        "name": "optimae",
                        "columns": [
                            {
                                "name": "optimizer_id",
                                "col_type": "integer"
                            },
                            {
                                "name": "optimum",
                                "col_type": "text"
                            },
                            {
                                "name": "optimum_hash",
                                "col_type": "string",
                                "length": 64
                            }
                        ]
                    }
                ]
            }
                        
        ]
    }
}