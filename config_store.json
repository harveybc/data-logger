{
    "store_plugin": "sqlite_store",
    "store_plugin_config": {
        "filename": "db.sqlite3",
        "processes" : [
            {
                "name": "feature_extractor",
                "tables": [
                    {
                        "name": "fe_config",
                        "columns": [
                            {
                                "name": "csv_file",
                                "col_type": "Integer" 
                            },
                            {
                                "name": "save_encoder",
                                "col_type": "save_decoder" 
                            },
                            {
                                "name": "save_decoder",
                                "col_type": "Boolean" 
                            },
                            {
                                "name": "load_encoder",
                                "col_type": "String" 
                            },
                            {
                                "name": "load_decoder",
                                "col_type": "String" 
                            },
                            {
                                "name": "evaluate_encoder",
                                "col_type": "Boolean" 
                            },
                            {
                                "name": "evaluate_decoder",
                                "col_type": "Boolean" 
                            },
                            {
                                "name": "encoder_plugin",
                                "col_type": "Boolean" 
                            },
                            {
                                "name": "decoder_plugin",
                                "col_type": "String" 
                            },
                            {
                                "name": "window_size",
                                "col_type": "String" 
                            },
                            {
                                "name": "max_error",
                                "col_type": "String" 
                            },
                            {
                                "name" : "initial_size",
                                "col_type" : "String"
                            },
                            {
                                "name": "step_size",
                                "col_type": "String"
                            },
                        ]
                    },
                    {
                        "name": "fe_training_error",
                        "columns": [
                            { "name" : "mse" },
                            { "name" : "mae" },
                            { "name" : "r2" },
                            { "name" : "interface_size" },
                            {
                                "name" : "config_id",
                                "foreign_key" : "fe_config.id"
                            }
                        ]
                    }
                ]
            }
        ]
    }
}